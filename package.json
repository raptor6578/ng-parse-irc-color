  },
  "author": "DAMM Nicolas",
  "license": "MIT",
  "devDependencies": {
    "@angular/compiler": "^7.2.4",
    "@angular/compiler-cli": "^7.2.4",
    "rollup": "^1.1.2",
    "typescript": "~3.2.2",
    "uglify-js": "^3.4.9"
  }
}
